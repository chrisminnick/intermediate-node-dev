{
  "name": "performance-analysis-lab",
  "version": "1.0.0",
  "description": "Node.js Performance Analysis and Optimization Lab",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "start:optimized": "node optimized/app.js",
    "profile": "node --inspect=0.0.0.0:9229 app.js",
    "profile:optimized": "node --inspect=0.0.0.0:9229 optimized/app.js",
    "clinic:flame": "clinic flame -- node app.js",
    "clinic:bubbleprof": "clinic bubbleprof -- node app.js",
    "clinic:heapprofiler": "clinic heapprofiler -- node app.js",
    "test:load": "autocannon -c 10 -d 30 http://localhost:3000/api/products",
    "test:search": "autocannon -c 10 -d 30 'http://localhost:3000/api/search?q=laptop'",
    "test:reports": "autocannon -c 10 -d 30 http://localhost:3000/api/reports/sales",
    "test:artillery": "artillery run load-test.yml",
    "benchmark": "node benchmarks/compare.js",
    "memory:watch": "node --max-old-space-size=2048 --trace-gc app.js",
    "setup": "npm install && node scripts/seed-data.js"
  },
  "keywords": [
    "nodejs",
    "performance",
    "optimization",
    "profiling",
    "benchmarking"
  ],
  "author": "Performance Lab",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "redis": "^4.6.5",
    "node-cache": "^5.1.2",
    "pg": "^8.10.0",
    "pg-pool": "^3.6.0",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "winston": "^3.8.2",
    "express-winston": "^4.2.0",
    "swagger-ui-express": "^4.6.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "clinic": "^12.0.0",
    "autocannon": "^7.10.0",
    "artillery": "^2.0.0",
    "0x": "^5.5.0",
    "memwatch-next": "^0.3.0",
    "heapdump": "^0.3.15",
    "jest": "^29.5.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
